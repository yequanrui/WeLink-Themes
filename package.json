{
  "name": "welink-themes",
  "productName": "WeLink-Themes",
  "version": "4.0.0",
  "description": "WeLink主题设置工具",
  "keywords": [
    "welink",
    "themes",
    "electron"
  ],
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@opentiny/vue-button": "^3.12.0",
    "@opentiny/vue-container": "^3.12.0",
    "@opentiny/vue-dropdown": "^3.12.0",
    "@opentiny/vue-dropdown-item": "^3.12.0",
    "@opentiny/vue-dropdown-menu": "^3.12.1",
    "@opentiny/vue-icon": "^3.12.0",
    "@opentiny/vue-link": "^3.12.0",
    "@opentiny/vue-popover": "^3.12.0",
    "@opentiny/vue-tooltip": "^3.12.0",
    "electron-updater": "^6.1.1",
    "vue-i18n": "^9.8.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^1.0.29",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "less": "^4.2.0",
    "prettier": "^3.1.1",
    "vite": "^4.5.1",
    "vue": "^3.3.12"
  },
  "license": "MIT",
  "author": "yequanrui",
  "homepage": "https://github.com/yequanrui/WeLink-Themes",
  "repository": {
    "type": "git",
    "url": "https://github.com/yequanrui/WeLink-Themes.git"
  },
  "bugs": {
    "url": "https://github.com/yequanrui/WeLink-Themes/issues"
  }
}
